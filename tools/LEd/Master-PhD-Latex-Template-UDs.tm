line:\newcommand{\myPrintChapterLabel}[1]{
line:	\ifthenelse{\equal{#1}{A}}
line:		{\myAppendixLabel} 
line:		{\ifthenelse{\equal{#1}{B}}
line:			{\myAppendixLabel} 
line:			{\ifthenelse{\equal{#1}{C}}
line:				{\myAppendixLabel} 
line:				{\ifthenelse{\equal{#1}{D}}
line:					{\myAppendixLabel} 
line:					{\ifthenelse{\equal{#1}{E}}
line:						{\myAppendixLabel} 
line:						{\ifthenelse{\equal{#1}{F}}
line:							{\myAppendixLabel} 
line:							{\ifthenelse{\equal{#1}{G}}
line:								{\myAppendixLabel} 
line:								{\myChapterLabel}}}}}}} 
line:}
